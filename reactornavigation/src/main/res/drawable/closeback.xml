<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@+id/close"
        android:drawable="@drawable/close"
        android:state_checked="true"/>

    <item
        android:id="@+id/back"
        android:drawable="@drawable/back"/>

    <transition
        android:drawable="@drawable/pathmorph_close_to_back"
        android:fromId="@id/close"
        android:toId="@id/back"/>

    <transition
        android:drawable="@drawable/pathmorph_back_to_close"
        android:fromId="@id/back"
        android:toId="@id/close"/>

</animated-selector>